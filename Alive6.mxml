			private function add2Minutes(t:String):String	{
				var hhmm:Array = t.split(":");
				var h:String = '';
				var m:String = '';
				
				if (parseInt(hhmm[1]) == 58)	{
					m = '00';
					if (parseInt(hhmm[0]) == 23)	{
						h = '00';
					}	else	{
						h = (parseInt(hhmm[0])+1).toString();
					}
				}	else	{
					m = (parseInt(hhmm[1]) + 2).toString();
					if (parseInt(hhmm[0]) == 23)	{
						h = '00';
					}	else	{
						h = hhmm[0];
					}					
				}
				if (h.length == 1)
					h = '0' + h;
				if (m.length == 1)
					m = '0' + m;
				return h+":"+m;
			}
			
