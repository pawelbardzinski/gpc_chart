			private function onTimer(event:TimerEvent):void {
				if ((weekend == "weekend") && (historyBtn.selected==false))	{
					if (WKD == false)	{
						WKD = true;
					}
					chartBkdLabel.text = 'The Market is Closed.';
					return;
				} 
				
					WKD = false;
					if (timer.delay != 1000) { 
						timer.delay = 1000; 
					}
				
					var mm:String; 
					var ss:String; 
	
					s_num--; 
					if ((s_num == -1) && (m_num > 0)) { 
						m_num--; 
						s_num = 59; 
					} 
					if (s_num < 0) { 
						if (!(overdueLabel.visible))	{
							s_num = 0; 
							timer.stop(); 
							chartBkdLabel.text = 'Next live price update in ';     
							chartBkdLabel2.visible = false;
							overdueLabel.visible = true;
							progressring.running = false;
							progressring.phase = 0;
							fader.play();
							tracker.trackPageview("Gold Price Waiting");
							return;
						}
					}
					mm =  (m_num < 10) ? "0" + m_num.toString() : m_num.toString(); 
					ss = (s_num < 10) ? "0" + s_num.toString() : s_num.toString(); 
					chartBkdLabel.text = 'Next live price update in ';
					chartBkdLabel2.text = mm+':'+ss;
			} 			
			
